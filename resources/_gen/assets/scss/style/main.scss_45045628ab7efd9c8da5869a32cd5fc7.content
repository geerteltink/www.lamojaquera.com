.app{display:flex;min-height:100vh;flex-direction:column;background-color:#fff}.app-hero{height:196px;height:calc(100vw/6.2 - 1px)!important;color:#fff;background:no-repeat 50%;background-size:cover}.app-hero-content{margin-left:2em;margin-right:2em}.app-content{flex:1}.app-footer,.app-footer a{color:#fff;background-color:#333;padding:.5rem 1rem}.navbar-brand{width:156px;position:relative}.navbar-brand-image{height:116px;position:absolute;top:-20px;z-index:100}.navbar .nav-item{font-size:1.4rem}.subnav-link{color:#15179e;font-weight:700}.subnav-link.active{color:#2788fe}.flex-container{display:flex;height:100%;align-items:center;justify-content:center}.timeline{list-style-type:none}.timeline-item{border-left:1px solid #e5e5e5;position:relative;padding:2rem 1.5rem .5rem 2.5rem;margin-left:1rem;min-height:5rem}.timeline-item:last-child{padding-bottom:4rem}.timeline-date{margin-bottom:.5rem}.timeline-date span{color:#888;font-size:85%;padding-left:.4rem}.timeline-content{padding:.5rem .8rem;background-color:#f4f4f4;border-radius:.5rem}.timeline-content span{display:block;color:#888;font-size:85%}.timeline-title{font-size:1.4rem}.timeline-location{color:#888}.timeline-icon{position:absolute;top:2.2rem;width:1rem;height:1rem;margin-left:-3rem;border-radius:50%;background:#042486;box-shadow:0 0 0 4px #fff,inset 0 2px rgba(0,0,0,8%),0 3px 0 4px rgba(0,0,0,5%)}.card-img-top{max-width:100%;flex-shrink:0}.galery-card{min-width:164px;max-width:512px;margin-bottom:1rem!important}.galery-card:hover{text-decoration:none}.galery-card-text{font-size:1rem;font-style:italic;font-weight:300}@media(min-width:576px){.timeline-item{margin-left:10rem}.timeline-date{position:absolute;left:-10rem;width:7.5rem;text-align:right}.timeline-date span{display:block}.timeline-content{padding:0;background-color:transparent}}.photocall{flex-flow:row wrap}.photocall-logo{min-width:148px;max-width:164px}.photocall-footer{padding:.5rem;white-space:nowrap;overflow-x:hidden;font-size:.64rem;text-align:ce