.app{display:flex;min-height:100vh;flex-direction:column;background-color:#fff}.app-hero{height:196px;height:calc(100vw/6.2 - 1px)!important;color:#fff;background:no-repeat 50%;background-size:cover}.app-hero-content{margin-left:2em;margin-right:2em}.app-content{flex:1}.app-footer,.app-footer a{color:#fff;background-color:#333;padding:.5rem 1rem}.header-video{position:relative;overflow:hidden}.raster-overlay{height:100%;width:100%;background-image:url(/img/raster_dot.png);background-repeat:repeat}.header-title{color:#fff;margin-left:1rem;padding:0 1.5rem;border-width:0 0 0 8px;border-style:solid;border-color:#73a147;text-shadow:0 0 8px #73a147}.header-title h1{line-height:1}.header-title p{margin-bottom:0}.header-caption{position:absolute;width:100%;z-index:40;top:50%;transform:translateY(-50%)}.navbar-brand{width:156px;position:relative}.navbar-brand-image{height:116px;position:absolute;top:-20px;z-index:100}.navbar .nav-item{font-size:1.4rem}.subnav-link{color:#15179e;font-weight:700}.subnav-link.active{color:#2788fe}.flex-container{display:flex;height:100%;align-items:center;justify-content:center}.video-viewport{position:relative;overflow:hidden;background-color:rgba(0,0,0,.25);background:url(/img/header-background.jpg) 50%;background-size:cover;width:100%;height:40vh}.embed-responsive{position:relative;display:block;height:0;padding:0;overflow:hidden;top:50%;left:50%;transform:translate(-50%,-50%);background:url(/img/header-background.jpg) 50%;background-size:cover;z-index:-10}.embed-responsive iframe,.embed-responsive embed,.embed-responsive video{position:absolute;width:100%;height:100%;border:0}.embed-responsive-16by9{padding-bottom:56.25%}.timeline{list-style-type:none}.timeline-item{border-left:1px solid #e5e5e5;position:relative;padding:2rem 1.5rem .5rem 2.5rem;margin-left:1rem;min-height:5rem}.timeline-item:last-child{padding-bottom:4rem}.timeline-date{margin-bottom:.5rem}.timeline-date span{color:#888;font-size:85%;padding-left:.4rem}.timeline-content{p